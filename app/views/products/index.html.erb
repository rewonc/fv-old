<h1>List of our products</h1>
 
<table>
  <tr>
    <th>Farm id</th>
    <th>Title</th>
    <th>Description</th>
    <th>Price</th>
  </tr>
 
  <% @products.each do |product| %>
    <tr>
      <td><%= product.farm_id %></td>
      <td><%= product.title %></td>
       <td><%= product.description %></td>
      <td><%= product.price %></td>
    </tr>
  <% end %>
</table>